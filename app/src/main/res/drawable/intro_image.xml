<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Outline or Border Layer -->
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#000000" />
            <padding
                android:top="2dp"
                android:right="2dp"
                android:bottom="2dp"
                android:left="2dp" />
        </shape>
    </item>
    <item
        android:drawable="@drawable/wallpaper_1"
        android:top="2dp"
        android:right="2dp"
        android:bottom="2dp"
        android:left="2dp">
    </item>
</layer-list>
